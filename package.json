{
  "name": "rollup-plugin-import-extendscript",
  "author": "Good Boy Ninja",
  "version": "1.0.4",
  "type": "module",
  "types": "./types/index.d.ts",
  "description": "import .jsx extendscript files as a string for later evaluation using evalScript()",
  "keywords": [
    "adobe",
    "extendscript",
    "jsx"
  ],
  "repository": {
    "url": "https://github.com/GoodBoyNinja/rollup-plugin-bundle-extendscript",
    "type": "git"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/rollup-plugin-import-extendscript.es.js",
      "require": "./dist/rollup-plugin-import-extendscript.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "path-browserify": "^1.0.1",
    "rollup-plugin-import-extendscript": "^1.0.0",
    "vite": "^4.1.0",
    "vite-plugin-node-polyfills": "^0.7.0"
  },
  "dependencies": {
    "code-stringify": "^2.0.3",
    "jsesc": "^3.0.2",
    "recursive-readdir": "^2.2.3",
    "rollup-pluginutils": "^2.8.2",
    "uuid": "^9.0.0"
  }
}
